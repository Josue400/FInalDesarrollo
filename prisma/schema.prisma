generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                 Int      @id @default(autoincrement())
  fecha              DateTime @default(now()) // Fecha de creaci칩n autom치tica
  lastname           String
  name               String
  genero             String
  fechanacimiento    String
  estado             String
  fecha_actualizacion DateTime @updatedAt // Fecha de actualizaci칩n autom치tica
  usuario            String
  email              String   @unique
}

